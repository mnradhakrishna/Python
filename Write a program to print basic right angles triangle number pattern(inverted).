#using functions 
def print_inverted_triangle():
    current_number = 10
    for i in range(1, 5):
        for j in range(1, 6 - i): 
            print(current_number, end=' ')
            current_number -= 1
            if current_number < 1: 
                break
        print()
        if current_number < 1:
            break
print_inverted_triangle()
#without using functions
rows = int(input("Enter number of rows: "))

for i in range(rows, 0, -1):
    for j in range(1, i+1):
        print(j, end=" ")
    
    print()
